{
  "type": "module",
  "browserslist": [
    "> 0.2%",
    "last 2 versions",
    "not dead",
    "Firefox ESR",
    "Chrome 61",
    "Firefox 60",
    "ios_saf 9"
  ],
  "scripts": {
    "start": "parcel serve src/index.html --no-cache",
    "build": "parcel build src/index.html --public-url /klecks/",
    "build:embed": "parcel build src/embed.ts",
    "build:help": "parcel build src/help.html --public-url .",
    "lang:add": "node src/languages/generate.js add",
    "lang:sync": "node src/languages/generate.js sync",
    "lang:build": "node src/languages/generate.js build",
    "deploy": "npm run lang:build && npm run build && gh-pages -d dist"
  },
  "devDependencies": {
    "gh-pages": "^6.0.0"
  },
  "dependencies": {
    "@parcel/transformer-glsl": "2.16.1",
    "@parcel/transformer-sass": "2.16.1",
    "@tldraw/tldraw": "^4.2.0",
    "ag-psd": "28.3.1",
    "buffer": "6.0.3",
    "js-beautify": "1.15.4",
    "json5": "2.2.3",
    "mdn-polyfills": "5.20.0",
    "parcel": "^2.16.1",
    "polygon-clipping": "0.15.7",
    "simplex-noise": "^4.0.3",
    "transformation-matrix": "3.1.0",
    "tsparticles": "^3.9.1"
  }
}
